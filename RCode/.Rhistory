exp_drift <- function(Mkt_ts, Mkt, strt){
#browser()
Mkta <- Mkt
cc <- Mkta[1,]
cc$a <- 0
ln <- nrow(Mkt)
for(i in strt:ln){
st <- i-30
Mkt_slice <- window(Mkt_ts,start=st,end=i)
modf <- rwf(Mkt_slice,drift=TRUE,h=1)
#fcastf <- forecast.ets(modf,h=1)
a <- as.numeric(modf$mean)
c1 <- Mkta[i,]
ab <- cbind(c1,a)
cc <- rbind(cc,ab)
}
cc <- cc[-1,]
return(cc)
}
