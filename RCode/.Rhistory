ts_1_2_fnc_ar <- function(fil,nm,ts1){
for(i in 1:length(fil)){
Mkt <- read.csv(fil[i],stringsAsFactors=F)
Mkt_p <- Mkt[,c(1,2,3,4,5,18)]
colnames(Mkt_p) <- c("Date","Open", "High","Low","Close","p")
if(ts1 == TRUE){
a <- ts_1(Mkt_p, 0, nm[i])
} else {
a <- ts_2(Mkt_p, 0, nm[i])
}
df10 <- rbind(df10, a)
}
df.name <- names(a)
names(df10) <- df.name
df10 <- df10[-c(1),]
return(df10)
}
