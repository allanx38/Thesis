main="Dax over 300 Days",
xlab="Day", ylab="",
xlim=c(220, 540))
lines(meanf(Mkt_pl, h=50) $mean, col=4)
lines(rwf(Mkt_pl,h=50)$mean,col=2)
lines(rwf(Mkt_pl,drift=TRUE,h=50)$mean,col=3)
